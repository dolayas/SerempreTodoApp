{
    "env": {
        "es6": true,
        "shared-node-browser": true
    },
    "extends": [],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaFeatures": {
            "arrowFunctions": true,
            "classes": true,
            "defaultParams": true,
            "destructuring": true,
            "experimentalObjectRestSpread": true,
            "forOf": true,
            "generators": true,
            "jsx": true,
            "modules": true,
            "objectLiteralComputedProperties": true,
            "restParams": true,
            "spread": true,
            "templateStrings": true
        },
        "ecmaVersion": 2020,
        "project": "./tsconfig.eslint.json",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "sort-keys-fix",
        "jsdoc",
        "import"
    ],
    "root": true,
    "rules": {
        "@typescript-eslint/indent": [
            "error",
            4
        ],
        "@typescript-eslint/quotes": [
            "error",
            "double",
            {
                "allowTemplateLiterals": true
            }
        ],
        "indent": "off",
        "quotes": "off",
        "sort-keys-fix/sort-keys-fix": "error"
    }
}